<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="workonweb_list" model="ir.ui.view">
        <field name="name">workon.web.list</field>
        <field name="model">workon.web</field>
        <field name="arch" type="xml">
            <list string="Workn web">
                <field name="name"/>
                <field name="std_class"/>
            </list>
        </field>
    </record>

    <record id="workonweb_form" model="ir.ui.view">
        <field name="name">workon.web.form</field>
        <field name="model">workon.web</field>
        <field name="arch" type="xml">
            <form string="Workon Web">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="std_class"/>
                    </group>
                </sheet>

            </form>

        </field>
    </record>


    <record id="action_workonweb" model="ir.actions.act_window">
        <field name="name">Work On Web</field>
        <field name="res_model">workon.web</field>
        <field name="view_mode">list,form</field>
    </record>



    <menuitem id="menu_workonweb" name="Work On Web" sequence="10"/>
    <menuitem id="work_on_work_forms" name="Work On Web" parent="menu_workonweb"
              action="action_workonweb"/>

</odoo>